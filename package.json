    "graphql",
    "relay",
    "catch",
    "validation",
    "cli",
    "rust"
  ],
  "author": "Jan Nicklas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jantimon/gql-safeguard.git"
  },
  "bugs": {
    "url": "https://github.com/jantimon/gql-safeguard/issues"
  },
  "homepage": "https://github.com/jantimon/gql-safeguard#readme",
  "engines": {
